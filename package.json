{
  "name": "sbgnviz-js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-sbgnviz-js": "gulp build",
    "build-app-js": "browserify main.js -o app.js --debug",
    "watch-sampleapp-js": "nodemon -e js --watch sample-app/js/ -x \"npm run build-sbgnviz-js\"",
    "watch-src-js": "nodemon -e js --watch src -x \"npm run build-sbgnviz-js\"",
    "watch-sbgnviz-js": "nodemon -e js --watch sbgnviz.js -x \"npm run build-app-js\"",
    "watch-main-js": "nodemon -e js --watch main.js -x \"npm run build-app-js\"",
    "watch-js": "parallelshell \"npm run watch-sampleapp-js\" \"npm run watch-src-js\" \"npm run watch-main-js\" \"npm run watch-sbgnviz-js\"",
    "release-build": "npm run build-sbgnviz-js && npm run build-app-js",
    "debug-build": "npm run watch-js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iVis-at-Bilkent/sbgnviz-js.git"
  },
  "bugs": {
    "url": "https://github.com/iVis-at-Bilkent/sbgnviz-js/issues"
  },
  "homepage": "https://github.com/iVis-at-Bilkent/sbgnviz-js/",
  "dependencies": {
    "jquery": "2.2.4",
    "jquery.browser": "0.1.0",
    "qtip2": "~3.0.2",
    "bootstrap": "3.3.7",
    "jquery-ui-bundle": "1.11.4",
    "jquery-expander": "1.7.0",
    "cytoscape-panzoom": "~2.4.0",
    "cytoscape-qtip": "~2.3.0",
    "filesaverjs": "~0.2.2",
    "backbone": "1.3.3",
    "underscore": "1.8.3",
    "cytoscape-cose-bilkent": "latest",
    "cytoscape-undo-redo": "latest",
    "cytoscape-expand-collapse": "latest",
    "cytoscape-clipboard": "latest",
    "cytoscape-edge-bend-editing": "latest",
    "cytoscape-view-utilities": "latest",
    "cytoscape-context-menus": "latest"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "gulp": "^3.9.0",
    "gulp-derequire": "^2.1.0",
    "gulp-util": "^3.0.6",
    "node-notifier": "^4.3.1",
    "run-sequence": "^1.1.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "nodemon": "latest",
    "parallelshell": "^2.0.0"
  }
}
